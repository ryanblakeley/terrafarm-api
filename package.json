{
  "name": "terrafarm-api",
  "private": true,
  "version": "3.0.0-a1",
  "description": "The Terrafarm API serves a REST endpoint and other utilities.",
  "repository": "rojobuffalo/terrafarm-api",
  "author": "rojobuffalo",
  "license": "Creative Commons BY-NC-ND 3.0",
  "keywords": [],
  "main": "gulpfile.babel.js",
  "scripts": {
    "setup": "cp -n .env.example .env || true",
    "rest": "postgrest ./postgrest.conf",
    "graphql": "gulp",
    "eslint": "eslint ./src",
    "test": "JWT_PRIVATE_KEY=foo NODE_ENV=test ./node_modules/mocha/bin/_mocha --opts ./spec/mocha.opts",
    "build": "rimraf build && gulp build",
    "start": "pm2 start ecosystem.json --env production",
    "restart": "pm2 restart ecosystem.json",
    "stop": "pm2 stop api",
    "deploy": ""
  },
  "engines": {
    "node": "6.10.0",
    "npm": "3.10.10"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "express": "^4.15.2"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-preset-airbnb": "^2.2.3",
    "chai": "3.5.0",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "express": "^4.13.3",
    "fetch-everywhere": "^1.0.5",
    "gulp": "^3.9.1",
    "gulp-env": "^0.4.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.10.2",
    "path": "^0.12.7",
    "sinon": "^1.17.7",
    "source-map-support": "^0.4.12",
    "webpack": "^1.13.2",
    "webpack-validator": "^2.2.7"
  }
}
